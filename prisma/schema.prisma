generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Score {
  id        Int      @id @default(autoincrement())
  name      String
  score     Int
  createdAt DateTime @default(now())
}

